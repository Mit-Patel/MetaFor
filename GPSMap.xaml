<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MetaFor_Demo"
        xmlns:WindowsPresentation="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation" x:Class="MetaFor_Demo.GPSMap"
        mc:Ignorable="d"
        Title="GPSMap" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <WindowsPresentation:GMapControl x:Name="map" DockPanel.Dock="Top" Loaded="map_Loaded" />
        </DockPanel>
    </Grid>
</Window>
